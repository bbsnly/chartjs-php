{
    "name": "bbsnly/chartjs-php",
    "description": "PHP wrapper for Chart.js library",
    "keywords": [
        "chartjs",
        "php"
    ],
    "homepage": "https://github.com/bbsnly/chartjs-php",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoliy Babushka",
            "email": "anatoliybabushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Bbsnly\\ChartJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}