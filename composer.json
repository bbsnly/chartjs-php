{
  "name": "bbsnly/chartjs-php",
  "description": "PHP wrapper for Chart.js library",
  "keywords": [
    "chartjs",
    "php"
  ],
  "homepage": "https://github.com/bbsnly/chartjs-php",
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Anatoliy Babushka",
      "email": "anatoliybabushka@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Chart\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}