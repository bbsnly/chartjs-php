{
    "name": "bbsnly/chartjs-php",
    "description": "Laravel helper for ChartJS 2.0",
    "keywords": [
        "chartjs",
        "php",
        "laravel"
    ],
    "homepage": "https://github.com/bbsnly/chartjs-php",
    "type": "package",
    "version": "3.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoliy Babushka",
            "email": "anatoliybabushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Bbsnly\\ChartJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "scripts": {
        "test": [
          "vendor/bin/phpunit"
        ]
    }
}
