{
    "name": "bbsnly/chartjs-php",
    "description": "PHP wrapper for Chart.js library",
    "keywords": [
        "chartjs",
        "php",
        "charts",
        "graphs",
        "data-visualization",
        "javascript",
        "chart-library",
        "statistics",
        "analytics",
        "php-library",
        "php-wrapper",
        "chart-generation"
    ],
    "homepage": "https://github.com/bbsnly/chartjs-php",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoliy Babushka",
            "email": "anatoliybabushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "psr-4": {
            "Bbsnly\\ChartJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
